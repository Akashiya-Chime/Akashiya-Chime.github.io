    <item>
      <title>用过的笔记软件汇总</title>
      <link>http://localhost:1313/posts/%E7%94%A8%E8%BF%87%E7%9A%84%E7%AC%94%E8%AE%B0%E8%BD%AF%E4%BB%B6%E6%B1%87%E6%80%BB/</link>
